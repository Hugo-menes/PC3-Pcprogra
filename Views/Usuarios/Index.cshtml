@model List<PC3_HUGO.Integration.models.Usuario>

@{
    ViewData["Title"] = "Lista de Usuarios";
}

<h2>Lista de Usuarios</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Avatar</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
            <tr>
                <td>@usuario.Id</td>
                <td>@usuario.Email</td>
                <td>@usuario.Nombre</td>
                <td>@usuario.Apellido</td>
                <td><img src="@usuario.Avatar" alt="Avatar" style="width: 50px; height: 50px;" /></td>
            </tr>
        }
    </tbody>
</table>
